ell>\r\n                                        <TableCell>{row.created_at}</TableCell>\r\n                                        <TableCell>\r\n                                            <Button variant=\"outlined\" color=\"primary\" onClick={this.handleClickOpen.bind(this,row.NoBarang)}>\r\n                                                Code\r\n                                            </Button>\r\n                                        </TableCell>\r\n                                    </TableRow>\r\n                                ))\r\n                        }\r\n                    </TableBody>\r\n                </Table>\r\n            </TableContainer>\r\n            <Dialog\r\n                open={this.state.dialogopen}\r\n                onClose={this.handleClose}\r\n                aria-labelledby=\"alert-dialog-title\"\r\n                aria-describedby=\"alert-dialog-description\">\r\n                {/* <DialogTitle id=\"alert-dialog-title\">{\"Use Google's location service?\"}</DialogTitle> */}\r\n                <DialogContent>\r\n                    <DialogContentText>\r\n                        <QRCode value={this.state.nobarang} />\r\n                    </DialogContentText>\r\n                </DialogContent>\r\n                <DialogActions>\r\n                    <Button onClick={this.handleClose} color=\"primary\">\r\n                    <ReactToPrint\r\n                                trigger={() => <a href=\"#\">Print</a>}\r\n                                content={() => this.componentRef}\r\n                                />\r\n                    </Button>\r\n                    <Button onClick={this.handleClose} color=\"primary\" autoFocus=\"autoFocus\">\r\n                        Close\r\n                    </Button>\r\n                </DialogActions>\r\n            </Dialog>\r\n            <div style={{ display: \"none\" }}>\r\n                <ComponentToPrint ref={(el) => (this.componentRef = el)} nobarang={this.state.n